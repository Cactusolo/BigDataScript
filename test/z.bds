#!/usr/bin/env bds

bams := "$HOME/sakshi_cloud/bams.txt"

println "Reading file list '$bams'"
for(string bam : bams.readLines() ) {
	s3Path := "s3://az-ngs-seqauto/analysis/$bam"
	localPath := "$HOME/seqauto/data/analysis/$bam"
	println "bam: $bam\n\ts3Path: '$s3Path'\n\tlocalPath: '$localPath'"
	if( localPath <- s3Path ) s3Path.download(localPath)
}
println "Done!"
