# FTP
d = "ftp://ftp.ensembl.org/pub/release-75/fasta/homo_sapiens/dna"
dd = d.dir('*.dna.toplevel.fa.gz')
println "\ndir=$d\ndir= $dd"

dd = d.dirPath('*.dna.toplevel.fa.gz')
println "\ndir=$d\ndirPath = $dd"
