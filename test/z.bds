class GrandParentClass {
      void oneMethod() { println('GrandParent class') }
}
 
class ParentClass extends GrandParentClass{
      void oneMethod() {
            super.oneMethod()
            println('Parent class')
      }
}
 
class ChildClass extends ParentClass {
      void oneMethod() {
            super.oneMethod()
            println('Child class')
      }
}
 
child := new ChildClass()
child.oneMethod()
