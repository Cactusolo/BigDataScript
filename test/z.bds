#!/usr/bin/env bds

tid := task echo hey
wait
print("This is task id --> "); print(tid); print("\n")
print("This is task exitCode() --> "); print(tid.exitCode()); print("\n")
print("This is task isDone() --> "); print(tid.isDone()); print("\n")
print("This is task stdout() --> "); print(tid.stdout()); print("\n")
print("This is task stderr() --> "); print(tid.stderr()); print("\n")

